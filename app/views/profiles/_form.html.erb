<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <form>
  <div class="form-group">
    <label for="name">Name:</label>
    <input type="name" class="form-control" id="name">
  </div>
  <div class="form-group">
    <label for="userid">User ID:</label>
    <input type="userid" class="form-control" id="user_id">
  </div>
  <div class="form-group">
    <label for="summary">Summary:</label>
    <input type="summary" class="form-control" id="summary">
  </div>
  <div class="form-group">
    <label for="age">Age:</label>
    <input type="age" class="form-control" id="age">
  </div>
  <div class="form-group">
    <label for="gender">Gender:</label>
    <input type="gender" class="form-control" id="gender">
  </div>
  <div class="form-group">
    <label for="weight">Weight:</label>
    <input type="weight" class="form-control" id="weight">
  </div>
  <div class="form-group">
    <label for="age">Goal:</label>
    <input type="goal" class="form-control" id="goal">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
<% end %>
